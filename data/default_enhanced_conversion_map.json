    "link_speed": {
      "display_name": "Link\nSpeed",
      "description": "Network link speed",
      "data_type": "string",
      "is_required": false,
      "is_key_field": false,
      "xlsx_mappings": [
        {
          "pattern": "Speed",
          "mapping_type": "exact",
          "priority": 100,
          "case_sensitive": false
        },
        {
          "pattern": "Link Speed",
          "mapping_type": "exact",
          "priority": 100,
          "case_sensitive": false
        },
        {
          "pattern": "(?i)speed|bandwidth",
          "mapping_type": "regex",
          "priority": 80,
          "case_sensitive": false
        }
      ],
      "api_mappings": [
        {
          "primary_path": "$.link.speed",
          "fallback_paths": ["$.speed", "$.bandwidth"],
          "transformation": "normalize_speed"
        }
      ],
      "validation_rules": {
        "pattern": "^(\\d+[GM]?|\\d+\\s*(Gbps|Mbps|GB|MB))$"
      },
      "ui_config": {
        "column_width": 80,
        "sortable": true,
        "filterable": true,
        "hidden": false
      },
      "transformations": ["normalize_speed"]
    },
