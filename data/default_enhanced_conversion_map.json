    "server_label": {
      "display_name": "Server\nName",
      "description": "Server or device name",
      "data_type": "string",
      "is_required": true,
      "is_key_field": true,
      "xlsx_mappings": [
        {
          "pattern": "Host Name",
          "mapping_type": "exact",
          "priority": 100,
          "case_sensitive": false
        },
        {
          "pattern": "Server",
          "mapping_type": "exact",
          "priority": 90,
          "case_sensitive": false
        },
        {
          "pattern": "(?i)server.*name|host.*name|device.*name",
          "mapping_type": "regex",
          "priority": 80,
          "case_sensitive": false
        }
      ],
      "api_mappings": [
        {
          "primary_path": "$.label",
          "fallback_paths": ["$.name", "$.hostname"],
          "transformation": null
        }
      ],
      "validation_rules": {
        "min_length": 1,
        "max_length": 255
      },
      "ui_config": {
        "column_width": 120,
        "sortable": true,
        "filterable": true,
        "hidden": false
      }
    },
