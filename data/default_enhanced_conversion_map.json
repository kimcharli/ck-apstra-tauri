  "transformation_rules": {
    "generate_interface_name": {
      "name": "Generate Interface Name",
      "description": "Generate interface name based on port number and speed",
      "rule_type": "dynamic",
      "conditions": {
        "input_type": "numeric_port",
        "has_speed_data": true
      },
      "logic": {
        "type": "function",
        "name": "generate_interface_name"
      },
      "priority": 100
    },
    "normalize_speed": {
      "name": "Normalize Speed",
      "description": "Normalize speed values to standard format",
      "rule_type": "dynamic",
      "conditions": null,
      "logic": {
        "type": "function",
        "name": "normalize_speed"
      },
      "priority": 80
    },
    "boolean_conversion": {
      "name": "Boolean Conversion",
      "description": "Convert various boolean representations to true/false",
      "rule_type": "dynamic",
      "conditions": null,
      "logic": {
        "type": "value_map",
        "mappings": {
          "yes": "true",
          "no": "false",
          "1": "true",
          "0": "false",
          "Y": "true",
          "N": "false",
          "TRUE": "true",
          "FALSE": "false"
        }
      },
      "priority": 60
    },
    "lag_mode_conversion": {
      "name": "LAG Mode Conversion",
      "description": "Convert LACP Yes/No values to lacp_active/static modes",
      "rule_type": "dynamic",
      "conditions": null,
      "logic": {
        "type": "value_map",
        "mappings": {
          "yes": "lacp_active",
          "no": "none",
          "y": "lacp_active",
          "n": "none",
          "YES": "lacp_active",
          "NO": "none",
          "Y": "lacp_active",
          "N": "none",
          "true": "lacp_active",
          "false": "none",
          "1": "lacp_active",
          "0": "none",
          "lacp": "lacp_active"
        }
      },
      "priority": 70
    },
    "trim_and_clean": {
      "name": "Trim and Clean",
      "description": "Remove extra whitespace and clean text",
      "rule_type": "dynamic",
      "conditions": null,
      "logic": {
        "type": "function",
        "name": "trim_whitespace"
      },
      "priority": 40
    }
  }
